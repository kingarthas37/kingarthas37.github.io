al;\n      }\n    }\n  }\n  \n  .menu {\n    \n    @include clearfix;\n //  @include gradient(#E64346,#E13335);\n    background: #e13335;\n    \n    \n    height: 40px;\n\n    .nav {\n      float: left;\n      line-height: 40px;\n      margin: 0;\n      a {\n        color: #fff;\n        font-size: 14px;\n        @include inlineBlock;\n        padding: 0 30px;\n        &:hover {\n          background: #a40000;\n          text-decoration: none;\n        }\n      }\n    }\n    \n  }\n  \n  .category {\n    \n    float: left;\n    width: 200px;\n    background: #cd2a2c;\n    position: relative;\n    z-index: 101;\n    \n    h3 {\n      height: 40px;\n      line-height: 40px;\n      margin: 0;\n      position: relative;\n      a {\n        font-size: 14px;\n        color: #fff;\n        font-weight: bold;\n        display: block;\n        padding-left: 20px;\n        i {\n          position: absolute;\n          top:14px;\n          right: 10px;\n          width: 12px;\n          height: 12px;\n          background: url('../images/ico-menu-arrow-down.png');\n        }\n        \n      }\n      \n    }\n    \n    \n    ul.layer {\n\n      display: none;\n      width: 196px;\n      border: 2px solid $red;\n      border-top: none;\n      background: #fafafa;\n      position: absolute;\n      \n      li {\n        height: 30px;\n        position: relative;\n        \n        h4 {\n          line-height: 30px;\n          padding-left: 10px;\n        }\n        \n        h4.hover {\n          display: none;\n          position: absolute;\n          height: 28px;\n          line-height: 28px;\n          top:0;\n          left:0;\n          border-top: 1px solid #ddd;\n          border-bottom: 1px solid #ddd;\n          width: 180px;\n          z-index: 2;\n          background: #fff;\n        }\n\n        div.layer {\n          display: none;\n          position: absolute;\n          z-index: 1;\n          width:400px;\n          padding: 10px 15px;\n          border: 1px soli